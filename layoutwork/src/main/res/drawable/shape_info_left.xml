<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!--背景色-->
    <solid android:color="#fff"/>
    <!--边框-->
    <stroke
        android:color="#fff"
        android:width="0.1dp"/>
    <!--大小宽高-->
    <size
        android:width="50dp"
        android:height="10dp"/>
    <!--边角-->
    <corners
        android:topLeftRadius="10dp"
        android:bottomLeftRadius="10dp"/>
</shape>